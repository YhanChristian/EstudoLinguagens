<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label id="lbLogin" layoutX="39.0" layoutY="93.0" prefHeight="42.0" prefWidth="197.0" text="Login:" textAlignment="JUSTIFY" textFill="RED">
               <font>
                  <Font name="Arial Bold" size="24.0" />
               </font>
            </Label>
            <TextField id="tfLogin" fx:id="tfLogin" layoutX="250.0" layoutY="92.0" prefHeight="43.0" prefWidth="256.0">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </TextField>
            <TextField id="tfSenha" fx:id="tfSenha" layoutX="250.0" layoutY="157.0" prefHeight="43.0" prefWidth="256.0">
               <font>
                  <Font name="Arial" size="24.0" />
               </font>
            </TextField>
            <Label id="lbSenha" layoutX="39.0" layoutY="158.0" prefHeight="42.0" prefWidth="197.0" text="Senha:" textAlignment="JUSTIFY" textFill="RED">
               <font>
                  <Font name="Arial Bold" size="24.0" />
               </font>
            </Label>
            <Button id="btnSubmit" fx:id="btnSubmit" contentDisplay="CENTER" layoutX="212.0" layoutY="252.0" mnemonicParsing="false" onAction="#submitLogin" prefHeight="69.0" prefWidth="216.0" text="Entrar" textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Button>
         </children>
    </AnchorPane>
  </children>
</VBox>
